<div class="row">
  <div class="col-12">
    <%= link_to 'Your Client page'.html_safe, '/client/home', method: :get, :class => "btn btn-primary" %>
  </div>
</div>
<div class="row">
  <div class="col-12">
    <div class="card">
      <div class="card-header header-elements-inline">
        <h5 class="card-title">Orders</h5>
      </div>
      <table id="searchable" class='table table-striped' data-url="<%= admin_orders_path(:format => :json) %>">
        <thead>
          <tr>
            <th>Email</th>
            <th>Status</th>
            <th>Type</th>
            <th>Topic</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody>

        </tbody>
      </table>
    </div>
  </div>
</div>

<%= render 'general/data_tables_scripts' %>
